import java.util.Scanner;
 
public class Main {
 
	public static void main(String[] args) {
 
		Scanner in = new Scanner(System.in);
		
		int a = in.nextInt();
		int b = in.nextInt();
		
		int[] coin = new int[a];
		
		for(int i = 0; i < a; i++) {
			coin[i] = in.nextInt();
		}
		
		int count = 0;
 
		for(int i = N - 1; i >= 0; i--) {
 
			if(coin[i] <= b) {
			
				count += (b / coin[i]);
				b = b % coin[i];
			}
		}
		System.out.println(count);
	}
 
}